doctype html
html(lang='en')
    head
        meta(charset='UTF-8')
        meta(name='viewport', content='width=device-width, user-scalable=no, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0')
        meta(name='description', content='NativeAI is an advertising and analytics platform for news publishers on web and mobile. It uses advanced personalization algorithms to help content creators understand their audience and predict which content would do well, as well as use those insights to target native advertising and monetize their properties more effectively.')
        meta(name='keywords', content='native advertising, native content, native marketing, content advertising, publisher content, promo content')
        meta(name='google-site-verification', content='im692VYYIe_XKs3WVlA9BgsbX6-EWc20R6Sps-bDAM0')
        meta(http-equiv='X-UA-Compatible', content='ie=edge')
        link(rel='apple-touch-icon', sizes='180x180', href='assets/favicon/apple-touch-icon.png')
        link(rel='icon', type='image/png', sizes='32x32', href='assets/favicon/favicon-32x32.png')
        link(rel='icon', type='image/png', sizes='16x16', href='assets/favicon/favicon-16x16.png')
        link(rel='manifest', href='assets/favicon/manifest.json')
        link(rel='mask-icon', href='assets/favicon/safari-pinned-tab.svg', color='#ff892a')
        link(rel='shortcut icon', href='assets/favicon/favicon.ico')
        meta(name='apple-mobile-web-app-title', content='NativeAI')
        meta(name='application-name', content='NativeAI')
        meta(name='msapplication-TileColor', content='#da532c')
        meta(name='msapplication-TileImage', content='assets/favicon/mstile-144x144.png')
        meta(name='msapplication-config', content='assets/favicon/browserconfig.xml')
        meta(name='theme-color', content='#ff892a')
        title NativeAI - Publisher Analytics & Monetization Platform
        link(rel='canonical', href='https://native.ai/')
        link(rel='stylesheet', href='style.css')
        link(href='https://fonts.googleapis.com/css?family=Ubuntu:300,400,400i,500,700', rel='stylesheet')
        script(src='https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js')
        script(src='js/app.min.js')
    body
        include ../sections/_header
        include ../sections/_first
        include ../sections/_analytics
        //- include ../sections/_power
        include ../sections/_brands
        include ../sections/_start
        include ../sections/_interests
        //- include ../sections/_case_study
        include ../sections/_how

        //- .homepage--six-screen
            .wrapper
                .title
                    h2 See How NativeAI Works for You
                .items-wrapper
                    .item-box.card
                        .img-section.orange-line
                            .title-box
                                h3.bold Publishers
                            img(src='assets/images/illustrations/home-publishers.svg')
                        .content-section
                            .text-box
                                p
                                    | Let us show you how easy it is to get deep intelligence on your audience, and how to use that intelligence to drive 20X engagement.
                            .button-box
                                a.button.orange(href='#') I Want a Demo
                    .item-box.card
                        .img-section.blue-line
                            .title-box
                                h3.bold Native Advertising
                            img(src='assets/images/illustrations/home-advertising.svg')
                        .content-section
                            .text-box
                                p
                                    | Get an Audience Match demonstration and e'll show you five insights inside of 15 minutes.
                            .button-box
                                a.button.blue(href='#') I Want a Demo

        include ../sections/_footer